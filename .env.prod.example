# Example production environment variables for BlaBlaBook V2
# Copy to .env.prod on the VPS and fill secrets. Do NOT commit .env.prod.

POSTGRES_DB=blablabook
POSTGRES_USER=blabla
POSTGRES_PASSWORD=changeme

DATABASE_URL=postgres://blabla:changeme@postgres:5432/blablabook

API_PORT=3000
TAG=1.0.0

JWT_SECRET=replace_with_a_long_random_secret
ALLOWED_ORIGINS=https://blablabook.online

# Cloudinary - Service d'h√©bergement d'images
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
CLOUDINARY_FOLDER=blablabookv2
